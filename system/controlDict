FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     pimpleDiffFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         14;

deltaT          0.001;

writeControl    adjustableRunTime;

writeInterval   0.5;

purgeWrite      0;

writeFormat     ascii;

writePrecision  7;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

adjustTimeStep  no; //yes;

maxCo           5;

///////////////////////////////////////////////////////////////////////////////

functions
{
	#includeFunc singleGraph
}

