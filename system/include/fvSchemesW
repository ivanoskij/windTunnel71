    div(phi,W4)	bounded Gauss upwind;
    div(phi,W5)	bounded Gauss upwind;
    div(phi,W6)	bounded Gauss upwind;
    div(phi,W7)	bounded Gauss upwind;
    div(phi,W8)	bounded Gauss upwind;
    div(phi,W9)	bounded Gauss upwind;

    div(phiW0,W0) bounded Gauss upwind;
    div(phiW1,W1) bounded Gauss upwind;
    div(phiW2,W2) bounded Gauss upwind;
    div(phiW3,W3) bounded Gauss upwind;
    div(phiW4,W4) bounded Gauss upwind;
    div(phiW5,W5) bounded Gauss upwind;
    div(phiW6,W6) bounded Gauss upwind;
    div(phiW7,W7) bounded Gauss upwind;
    div(phiW8,W8) bounded Gauss upwind;
    div(phiW9,W9) bounded Gauss upwind;
}
